{
  "name": "@datachonk/cli",
  "version": "0.2.1",
  "description": "AI-powered dbt expert CLI - analyze, generate, and optimize your dbt projects",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "datachonk": "./dist/index.js",
    "dchonk": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "prepublishOnly": "npm run build && npm run test"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "dbt",
    "data",
    "analytics",
    "sql",
    "cli",
    "ai",
    "data-modeling",
    "snowflake",
    "bigquery",
    "redshift",
    "databricks"
  ],
  "author": "DataChonk",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/llschmidt/datachonk-cli"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^12.0.0",
    "glob": "^10.3.10",
    "inquirer": "^9.2.15",
    "js-yaml": "^4.1.0",
    "open": "^10.0.0",
    "ora": "^8.0.1",
    "sql-formatter": "^15.3.0",
    "table": "^6.8.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.24",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
